]0;cse1100@brokolee: ~[01;32mcse1100@brokolee[00m:[01;34m~[00m$ bash 3-3.sh[1@gedit[C[C[C[C[C[C[C[1Pbash 3-2.sh[1Pcat[C[C[C[C[C[C[C[1@bash[C[C[C[C[C[C[C[1Pcat[C[C[C[C[C[C[C[1P.sh3.sh
#!/bin/bash
read -p "Enter a word" str
if [ "$str" == $(echo "$str" |rev) ]; then
	echo Yes, it is a palindrome!
else
	echo No, it is not a palindrome.
fi

